<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=window">
         <menu
               label="RLExtensions">
            <command
                  commandId="Pattern01.crudConfigCommand"
                  label="Crud configuration"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer?after=additions">
         <menu
               id="Pattern01.menu1"
               label="Yogurt"
               path="popup:org.eclipse.ui.views.ResourceNavigator?after=additions">
            <command
                  commandId="Pattern01.classCollectorCommand"
                  label="Class Collector"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="pattern01.plugin.components.navigator.PatternNavigator"
            id="Pattern01.NavigatorView"
            name="Pattern Navigator"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="pattern01.popup.commands.ClassCollectorHandler"
            commandId="Pattern01.classCollectorCommand">
      </handler>
      <handler
            class="pattern01.popup.commands.PatternEditorHandler"
            commandId="Pattern01.editPatternCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="pattern01.plugin.components.editors.generated.PatternInstancePatternEditor"
            default="false"
            id="pattern01.plugin.components.editors.generated.PatternInstanceEditorPart"
            name="PatternInstanceEditorPart">
      </editor>
   </extension>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="pattern01.plugin.components.editors.generated.PatternInstancePatternEditor"
			 default="false"
			 id="pattern01.plugin.components.editors.generated.PatternInstancePatternEditor"
			 name="PatternInstancePatternEditor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="pattern01.plugin.components.editors.generated.RelatedFilesPatternEditor"
			 default="false"
			 id="pattern01.plugin.components.editors.generated.RelatedFilesPatternEditor"
			 name="RelatedFilesPatternEditor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="pattern01.plugin.components.editors.generated.RelatedFilePatternEditor"
			 default="false"
			 id="pattern01.plugin.components.editors.generated.RelatedFilePatternEditor"
			 name="RelatedFilePatternEditor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="pattern01.plugin.components.editors.generated.FacesPatternEditor"
			 default="false"
			 id="pattern01.plugin.components.editors.generated.FacesPatternEditor"
			 name="FacesPatternEditor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="pattern01.plugin.components.editors.generated.SelectionPatternEditor"
			 default="false"
			 id="pattern01.plugin.components.editors.generated.SelectionPatternEditor"
			 name="SelectionPatternEditor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="pattern01.plugin.components.editors.generated.RegisterPatternEditor"
			 default="false"
			 id="pattern01.plugin.components.editors.generated.RegisterPatternEditor"
			 name="RegisterPatternEditor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="pattern01.plugin.components.editors.generated.DialogPatternEditor"
			 default="false"
			 id="pattern01.plugin.components.editors.generated.DialogPatternEditor"
			 name="DialogPatternEditor">
		</editor>
	</extension>
</plugin>
